{
  "name": "@koa/static",
  "description": "Static file serving middleware for koa",
  "version": "6.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "koa",
    "middleware",
    "file",
    "static",
    "sendfile"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/koajs/static.git"
  },
  "author": {
    "name": "TJ Holowaychuk",
    "email": "tj@apex.sh",
    "url": "https://apex.sh/"
  },
  "contributors": [
    {
      "name": "Imed Jaberi",
      "email": "imed_jebari@hotmail.fr"
    },
    {
      "name": "Evgenii Troinov",
      "email": "troinof@gmail.com"
    }
  ],
  "devDependencies": {
    "@swc/core": "^1.3.83",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.4",
    "@types/koa": "^2.13.8",
    "@types/node": "^20.6.0",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.49.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-xo-lass": "^2.0.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.4",
    "koa": "^2.14.2",
    "supertest": "^6.3.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "xo": "^0.56.0"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.4",
    "@koa/send": "^5.0.1"
  },
  "scripts": {
    "build": "tsup",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "jest --detectOpenHandles",
    "test-ci": "npm run test -- --coverage",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">= 18"
  }
